<% @listings.each do |listing| %>
  <% link_to listing_path(listing), :action => 'go' do %>
  <div class="card", style="background-image: linear-gradient(180deg, rgba(0, 0, 0, 0.62) 14.36%, rgba(0, 0, 0, 0) 60.22%)">
    <%= cl_image_tag listing.photos.first.content, :gravity=>"auto", :height=>1143, :quality=>"auto", :width=>2763, :crop=>"fill" %>
      <div class="card-description">
        <div class="card-description-h2"><h2 class="m-0"><%=listing.name%></h2></div>
        <div class="card-description-p"><p><%=listing.location%></p></div>
      </div>
      <div class="card-share-listing"><!--Hamburger menu--></div>
  </div>
  <% end %>
<% end %>


